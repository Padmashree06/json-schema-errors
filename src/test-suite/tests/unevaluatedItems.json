{
  "description": "unevaluatedItems keyword",
  "tests": [
    {
      "description": "unevaluatedItems with false schema",
      "compatibility": "2020",
      "schema": {
        "allOf": [
          {
            "prefixItems": [{ "type": "number" }]
          }
        ],
        "unevaluatedItems": false
      },
      "instance": [42, "foo"],
      "errors": [
        {
          "messageId": "boolean-schema-message",
          "messageParams": {},
          "instanceLocation": "#/1",
          "schemaLocations": ["#/unevaluatedItems"]
        }
      ]
    },
    {
      "description": "unevaluatedItems with object schema",
      "compatibility": "2020",
      "schema": {
        "allOf": [
          {
            "prefixItems": [{ "type": "number" }]
          }
        ],
        "unevaluatedItems": { "type": "boolean" }
      },
      "instance": [42, "foo"],
      "errors": [
        {
          "messageId": "type-message",
          "messageParams": {
            "expectedTypes": { "or": ["boolean"] }
          },
          "instanceLocation": "#/1",
          "schemaLocations": ["#/unevaluatedItems/type"]
        }
      ]
    },
    {
      "description": "unevaluatedItems with contains",
      "compatibility": "2019",
      "schema": {
        "allOf": [
          {
            "contains": { "type": "string" }
          }
        ],
        "unevaluatedItems": { "type": "number" }
      },
      "instance": [42, "foo", true],
      "errors": [
        {
          "messageId": "type-message",
          "messageParams": {
            "expectedTypes": { "or": ["number"] }
          },
          "instanceLocation": "#/2",
          "schemaLocations": ["#/unevaluatedItems/type"]
        }
      ]
    },
    {
      "description": "unevaluatedItems on a non-array",
      "compatibility": "2020",
      "schema": {
        "allOf": [
          {
            "prefixItems": [{ "type": "number" }]
          }
        ],
        "unevaluatedItems": { "type": "string" }
      },
      "instance": 42,
      "errors": []
    },
    {
      "description": "unevaluatedItems pass",
      "compatibility": "2020",
      "schema": {
        "allOf": [
          {
            "prefixItems": [{ "type": "number" }]
          }
        ],
        "unevaluatedItems": { "type": "string" }
      },
      "instance": [42, "foo"],
      "errors": []
    }
  ]
}
